    <OutputType>Exe</OutputType>
    <OutputPath>$(OutputPath)\..\AutopilotRelease\CommonSetup</OutputPath>
    <PostBuildEvent>del $(OutDir)\extracted.txt</PostBuildEvent>
  </PropertyGroup>
  
  <ItemGroup>
